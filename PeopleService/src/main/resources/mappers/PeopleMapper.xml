<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"
        "http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">
<mapper namespace="com.example.people.mapper.PeopleMapper">
    <resultMap id="peopleResultMap" type="com.example.feignapi.pojo.People">
        <result property="id" column="id" />
    </resultMap>
    <select id="getById" resultMap="peopleResultMap">
        select *
        from people
        where id = #{id}
    </select>

    <select id="list" resultMap="peopleResultMap">
        select * from people;
    </select>

</mapper>